#!/usr/bin/env bash
: "${SCRIPTS_PATH:="$(dirname $(dirname $(realpath "$BASH_SOURCE[0]")))"}"
# shellcheck disable=SC1091
source "$SCRIPTS_PATH/common.sh"

INIT_OPERATIONS_REQUIRED=(
    celery
    config
    connections
    dags
    db
    jobs
    kubernetes
    pools
    providers
    roles
    tasks
    users
    variables
)

source "$SCRIPTS_PATH/image/prepare_airflow_env.sh" || exit $?

: "${ZAIRFLOW_AIRFLOW_COMMAND_PATH="/usr/local/bin/airflow"}"

"$ZAIRFLOW_AIRFLOW_COMMAND_PATH" "$@" || exit $?
